<head>
  <meta charset="utf8">
  <title></title>
  <link rel="stylesheet" href="/normalize.css" type="text">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
 <div class="heading1">
<h2>People Greeted</h2>
<form action="/" method="get">
 <button class="button-primary homeBtn" type="submit" name="HomeButton">Home</button>
</form>
<table>
    <tr>
        <th>
            Names 
        </th>
        <th>
            language
        </th>
         <th>
            Counter
        </th>
    </tr>
    {{#each usersGreeted}}
    <tr>
        <td>
        <a href="/users/{{name}}">{{name}}</a> 
        </td>
         <td>
           {{language}} 
        </td>
        <td>
         {{count}}
        </td>
    </tr>
    {{/each}}
</table>
</div>